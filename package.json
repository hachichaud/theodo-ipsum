{
  "name": "boilerplate",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "bower install && gulp build",
    "start": "gulp watch",
    "test": "karma start test/karma-unit.coffee --single-run",
    "test:unit": "karma start test/karma-unit.coffee",
    "test:functional": "webdriver-manager update && protractor --require coffee-script/register test/protractor.conf.coffee",
    "docs": "gulp docs"
  },
  "dependencies": {
    "bower": "*",
    "coffee-script": "*",
    "gulp": "*",
    "gulp-angular-templatecache": "*",
    "gulp-coffee": "*",
    "gulp-concat": "*",
    "gulp-jade": "*",
    "gulp-plug": "*",
    "gulp-rename": "*",
    "gulp-rimraf": "*",
    "gulp-sass": "*",
    "gulp-sourcemaps": "*",
    "gulp-webserver": "*"
  },
  "devDependencies": {
    "chai": "*",
    "cucumber": "*",
    "dgeni": "*",
    "dgeni-packages": "*",
    "karma": "*",
    "karma-cli": "*",
    "karma-coffee-preprocessor": "*",
    "karma-coverage": "*",
    "karma-firefox-launcher": "*",
    "karma-mocha": "*",
    "karma-phantomjs-launcher": "*",
    "karma-sinon-chai": "*",
    "protractor": "*",
    "sinon": "*"
  }
}
